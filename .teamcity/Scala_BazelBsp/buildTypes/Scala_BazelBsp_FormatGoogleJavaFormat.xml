<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>[format] google java format</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="checking formatting with google java format" type="simpleRunner">
        <parameters>
          <param name="plugin.docker.imageId" value="vandmo/google-java-format" />
          <param name="plugin.docker.imagePlatform" value="linux" />
          <param name="plugin.docker.pull.enabled" value="true" />
          <param name="script.content" value="google-java-format -i --set-exit-if-changed $(find . -type f -name &quot;*.java&quot;)" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Scala_BazelBsp_BazelBspVcs" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

